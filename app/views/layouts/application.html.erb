<!DOCTYPE html>
<html>
  <head>
    <title>ThousandShop</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://maps.google.com/maps/api/js?key=#{ENV['GMAP_KEY']}" %>
    <%= javascript_include_tag '//cdn.jsdelivr.net/gmaps4rails/2.1.2/gmaps4rails.js' %>
    <%= javascript_include_tag '//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore.js' %>
    <%= javascript_include_tag 'https://cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js' %>
    <%= javascript_include_tag 'https://cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
